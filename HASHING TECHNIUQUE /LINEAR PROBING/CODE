#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define SIZE 10

// Linked list node
struct Node {
    char data[50];
    struct Node* next;
};

// Hash table
struct Node* table[SIZE];

// Hash function
int hash(char *str) {
    int sum = 0;
    for (int i = 0; str[i] != '\0'; i++)
        sum += str[i];
    return sum % SIZE;
}

// Insert into hash table
void insert(char *str) {
    int index = hash(str);
    struct Node* newNode = (struct Node*)malloc(sizeof(struct Node));
    strcpy(newNode->data, str);
    newNode->next = table[index];
    table[index] = newNode;
}

// Display hash table
void display() {
    for (int i = 0; i < SIZE; i++) {
        printf("%d: ", i);
        struct Node* temp = table[i];
        while (temp) {
            printf("%s -> ", temp->data);
            temp = temp->next;
        }
        printf("NULL\n");
    }
}

int main() {
    int n;
    char str[50];
    printf("Enter number of elements: ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        printf("Enter string %d:\n", i + 1);
        scanf("%s", str);
        insert(str);
    }

    printf("\nHash Table:\n");
    display();
    return 0;
}
