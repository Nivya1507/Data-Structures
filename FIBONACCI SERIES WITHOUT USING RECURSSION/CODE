#include <stdio.h>

int main() {
    int n, i;
    unsigned long long a = 0, b = 1, next;

    printf("Enter the number of terms: ");
    scanf("%d", &n);

    printf("Fibonacci Series: ");
    for (i = 1; i <= n; i++) {
        printf("%llu ", a);
        next = a + b;
        a = b;
        b = next;
    }

    return 0;
}
